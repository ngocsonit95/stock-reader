{
    "manifest_version": 3,
    "name": "Stock Voice Reader",
    "version": "1.0",
    "description": "Đọc lệnh khớp chứng khoán trực tiếp từ bảng giá",
    "permissions": [
        "tts"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://fireant.vn/*",
                "https://banggia.vps.com.vn/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}